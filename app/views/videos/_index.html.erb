<% videos.in_groups_of(3) do |group| %>
  <div class="row">
    <% group.each do |video| %>
      <% if video %>
        <div class="col-sm-4">
          <%= render 'video', video: video %>
        </div>
      <% end %>
    <% end %>
  </div>
<% end %>

<%= will_paginate videos, renderer: BootstrapPagination::Rails %>
